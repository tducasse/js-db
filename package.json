{
  "name": "@tducasse/js-db",
  "version": "0.2.2",
  "description": "A very tiny js in-memory database, using native JavaScript objects",
  "main": "dist/cjs/index.js",
  "types": "types/index.d.ts",
  "module": "dist/esm/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "tsd": "tsd",
