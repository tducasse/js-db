  "repository": {
    "type": "git",
    "url": "git+https://github.com/tducasse/js-db.git"
  },
  "keywords": [],
  "author": "tducasse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tducasse/js-db/issues"
  },
  "homepage": "https://github.com/tducasse/js-db#readme",
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "@types/jest": "26.0.24",
    "babel-jest": "26.6.3",
    "coveralls": "3.1.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-prettier": "3.4.1",
    "jest": "26.6.3",
    "prettier": "2.1.2",
    "rollup": "2.28.2",
    "rollup-plugin-terser": "7.0.2",
    "tsd": "0.13.1"
