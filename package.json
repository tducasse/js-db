    "url": "https://github.com/tducasse/js-db/issues"
  },
  "homepage": "https://github.com/tducasse/js-db#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@types/jest": "^26.0.14",
    "babel-jest": "^26.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "rollup": "^2.28.2",
    "rollup-plugin-ignore": "^1.0.10",
